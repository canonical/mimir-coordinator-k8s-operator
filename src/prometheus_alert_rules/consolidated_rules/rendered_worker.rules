      juju_model: WcTIWFaQj7vorWcmLfkq
      juju_model_uuid: 43df3144-ae12-47bd-83f8-9ed29b391e3f
      severity: warning
  - alert: MimirCompactorSkippedUnhealthyBlocks
    annotations:
      message: Mimir Compactor {{ $labels.pod }} in {{ $labels.cluster }}/{{ $labels.namespace
