      # Only if some compactions have started. We don''t want to fire this alert if
      the compactor has nothing to do

      # (e.g. there are more replicas than required because running as part of mimir-backend).

      (sum by(cluster, namespace, pod) (rate(cortex_compactor_group_compaction_runs_started_total[24h]))
      > 0)

      '
    for: 24h
    labels:
      juju_application: worker
      juju_charm: mimir
      juju_model: WcTIWFaQj7vorWcmLfkq
      juju_model_uuid: 43df3144-ae12-47bd-83f8-9ed29b391e3f
      severity: critical
      time_period: since-start
