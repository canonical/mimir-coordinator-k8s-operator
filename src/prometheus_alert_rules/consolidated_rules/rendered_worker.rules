- name: WcTIWFaQj7vorWcmLfkq_43df3144_worker_mimir_autoscaling_alerts
  rules:
  - alert: MimirAutoscalerNotActive
    annotations:
      message: The Horizontal Pod Autoscaler (HPA) {{ $labels.horizontalpodautoscaler
        }} in {{ $labels.namespace }} is not active.
      runbook_url: https://grafana.com/docs/mimir/latest/operators-guide/mimir-runbooks/#mimirautoscalernotactive
    expr: "(\n    label_replace((\n      kube_horizontalpodautoscaler_status_condition{condition=\"\
      ScalingActive\",status=\"false\"}\n      # Match only Mimir namespaces.\n  \
      \    * on(cluster, namespace) group_left max by(cluster, namespace) (cortex_build_info)\n\
      \      # Add \"metric\" label.\n      + on(cluster, namespace, horizontalpodautoscaler)\
      \ group_right label_replace(kube_horizontalpodautoscaler_spec_target_metric*0,\
      \ \"metric\", \"$1\", \"metric_name\", \"(.+)\")\n      > 0),\n      \"scaledObject\"\
      , \"$1\", \"horizontalpodautoscaler\", \"keda-hpa-(.*)\"\n    )\n)\n# Alert\
      \ only if the scaling metric exists and is > 0. If the KEDA ScaledObject is\
      \ configured to scale down 0,\n# then HPA ScalingActive may be false when expected\
      \ to run 0 replicas. In this case, the scaling metric exported\n# by KEDA could\
      \ not exist at all or being exposed with a value of 0.\nand on (cluster, namespace,\
      \ metric, scaledObject)\n(label_replace(keda_scaler_metrics_value, \"namespace\"\
      , \"$0\", \"exported_namespace\", \".+\") > 0)\n"
    for: 1h
    labels:
      juju_application: worker
      juju_charm: mimir
      juju_model: WcTIWFaQj7vorWcmLfkq
      juju_model_uuid: 43df3144-ae12-47bd-83f8-9ed29b391e3f
      severity: critical
  - alert: MimirAutoscalerKedaFailing
    annotations:
      message: The Keda ScaledObject {{ $labels.scaledObject }} in {{ $labels.namespace
        }} is experiencing errors.
      runbook_url: https://grafana.com/docs/mimir/latest/operators-guide/mimir-runbooks/#mimirautoscalerkedafailing
    expr: "(\n    # Find KEDA scalers reporting errors.\n    label_replace(rate(keda_scaler_errors[5m]),\
      \ \"namespace\", \"$1\", \"exported_namespace\", \"(.*)\")\n    # Match only\
      \ Mimir namespaces.\n    * on(cluster, namespace) group_left max by(cluster,\
      \ namespace) (cortex_build_info)\n)\n> 0\n"
    for: 1h
    labels:
      juju_application: worker
      juju_charm: mimir
      juju_model: WcTIWFaQj7vorWcmLfkq
      juju_model_uuid: 43df3144-ae12-47bd-83f8-9ed29b391e3f
      severity: critical
